@{
    ViewData["Title"] = "Index";
}

<h1>Utilizadores</h1>


<h3>Proprietários</h3>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                Nome
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.listaProp)
        {

            <tr>
                <td>
                    @item.UserName
                </td>
            </tr>
        }
    </tbody>
</table>
<h3>Clientes</h3>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                Nome
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.listaClis)
        {

            <tr onclick="location.href = '@(Url.Action("Details", "Utilizadores",new { id = item.Id }))'">
                <td>
                    @item.UserName
                </td>
            </tr>
        }
    </tbody>
</table>